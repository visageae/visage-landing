                                        <p>{{copy}}</p>
                                        <span class="testimonials__slide-name">{{name}}</span>
                                        <span class="testimonials__slide-job">{{job}}</span>
                                    </div>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with footer}}
    <footer class="footer">
        <div class="container">
            <div class="footer-wrapper">
                <div class="footer-left">
                    <img src="../assets/img/logo-gray.svg">
                    <h3>visage.jobs 2019</h3>
                    <span>visage.jobs 2019</span>
                    <span>Terms & conditions • Privacy Policy</span>
                </div>
                <div class="footer-right">
                    {{#each column}}
                        <div class="footer-right__col">
