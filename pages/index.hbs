</section>


{{#with testimonials}}
    <section class="main-testimonials">
        <div class="container">
            <div class="main-testimonials__wrapper">
                <div class="main-testimonials__header">
                    <h2>{{header}}</h2>
                    <hr>
                </div>
                <div class="main-testimonials__content">
                    <div class="main-testimonials__slider-wrap" js-testimonials-slider>
                        {{#each list_of_testimonials}}
