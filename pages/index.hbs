{{#with homepagehero}}
    <section id="homepage-hero" class="homepage-hero">
        <div class="homepage-hero__bg"></div>
        <div class="container">
            <div class="homepage-hero__contents">
                <div class="homepage-hero__info">
                    <div class="homepage-hero__info-wrap">
                        <h1 class="homepage-hero__title">{{ header }}</h1>
                        <h3 class="homepage-hero__subtitle">
                            Our crowdSOURCING Platform combines the expertise of 3,500 recruiting
                            sourcers and unique algorithms to deliver
                            <strong>high-quality candidates profiles to your company within 24
                                hours</strong>
                        </h3>
                        <a class="btn-round btn-primary-dark"
                           href="#"
                           aria-label="Request a demo"
                           data-micromodal-trigger="demo-request">
                            Request a Demo
                        </a>
                    </div>
                </div>
                <div class="homepage-hero__visual">
                    <img class="homepage-hero__illustrastion" src="{{ featured_image_url }}">
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with homepage_features}}
    <section id="features-showcase" class="features-showcase">
        <div class="container">
            <div class="features-showcase__head">
                <h2 class="features-showcase__title">Leverage the power of the crowd</h2>
                <div class="features-showcase__statement-wrapper">
                    {{#each statements}}
                        <div class="features-showcase__statement">
                            <span>{{header}}</span>
                            <p>{{copy}}</p>
                        </div>
                    {{/each}}
                </div>
            </div>
            <div class="features-showcase__contents" js-features-slider>
                {{#each list_of_features}}
                    <div class="features-showcase__tile-wrap">
                        <div class="features-showcase__tile">
                            <span class="features-showcase__tile-number">{{number}}</span>
                            <img class="features-showcase__tile-icon" src="{{icon_url}}">
                            <h3 class="features-showcase__tile-title">{{title}}</h3>
                            <p class="features-showcase__tile-copy">{{copy}}</p>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </section>
{{/with}}

{{#with main_advantages}}
    <section class="main-advantages">
        <div class="container">
            <h2 class="main-advantages__heading">{{header}}</h2>
            <div class="main-advantages__content">
                <div class="main-advantages__features">
                    {{#each list_of_advantages}}
                        <div class="main-advantages__features-item">
                            <div class="main-advantages__features-img">
                                <img src="{{img_url}}">
                            </div>
                            <div class="main-advantages__features-text">
                                <h3>{{header}}</h3>
                                <p>{{copy}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
                <div>
                    <img src="{{featured_img_url}}" class="main-advantages__image">
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with main_showcase}}
    <section class="main-showcase">
        <div class="container">
            <div class="main-showcase__wrapper">
                <div class="main-showcase__demo">
                    <h2>{{left_header}}</h2>
                    <a class="btn-sm-round btn-primary-dark"
                       href="#"
                       data-micromodal-trigger="demo-request"
                       aria-label="Request a demo">
                        Request a Demo
                    </a>
                </div>
                <div class="main-showcase__info">
                    <div class="main-showcase__info-text">
                        <h2>{{middle_header}}</h2>
                        <hr class="header-hr">
                        <p>{{middle_text}}</p>
                    </div>
                </div>
                <div class="main-showcase__features">
                    <div class="main-showcase__features-item">
                        <h2>{{features_header}}</h2>
                        <hr class="header-hr">
                    </div>
                    {{#each list_of_features}}
                        <div class="main-showcase__features-item">
                            <img src="{{icon_url}}">
                            <p>{{copy}}</p>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with main_users}}
    <section class="main-users">
        <div class="container">
            <div class="main-users__wrapper">
                <div class="main-users__heading">
                    <h2>{{header}}</h2>
                </div>
                <div class="main-users__content">
                    {{#each list_of_users}}
                        <div class="main-users__item">
                            <div class="main-users__item-img">
                                <img src="{{img_url}}">
                            </div>
                            <div class="main-users__item-text">
                                <h3>{{header}}</h3>
                                <p>{{copy}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with our_clients}}
    <section class="clients-showcase">
        <div class="container">
            <div class="clients-showcase__wrapper">
                <div class="clients-showcase__heading">
                    <h2>{{header}}</h2>
                </div>
                <div class="clients-showcase__content">
                    {{#each list_of_clients}}
                        <img class="clients-showcase__item" src="{{img_url}}">
                    {{/each}}
                </div>
            </div>
        </div>
    </section>
{{/with}}

{{#with company_members}}
    <section class="members">
        <div class="container">
            <div class="members__team">
                {{#each list_of_members}}
                    <div class="members__team-tile">
                        <div class="members__tile-img">
                            <img src="{{img_url}}">
                            <a class="members__tile-link" href="{{link}}">
                                <img src="../assets/img/linkedin-logo.svg">
                            </a>
                        </div>
                        <div class="members__tile-data">
                            <h3>{{name}}</h3>
                            <span>{{job}}</span>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </section>
{{/with}}

{{#with accordion}}
    <section class="faq">
        <div class="container">
            <h2 class="faq__heading">FAQs</h2>
            <div class="faq__wrapper">
                <div class="faq__accordion">
                    {{#each panel}}
                        <button class="accordion">{{out}}</button>
                        <div class="panel">
                            <p>{{in}}</p>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </section>
{{/with}}

