{{#with about_hero}}
    <section id="about-hero" class="about-hero">
        <div class="hero-overlay"></div>
        <div class="about-hero__bg"></div>
        <div class="container about-container">
            <div class="about-hero__contents">
                <div class="about-hero__info">
                    <div class="about-hero__info-wrap">
                        <h1 class="about-hero__title">{{header}}</h1>
                        <a class="btn-round btn-primary-dark"
                           href="#"
                           aria-label="Contact us"
                           data-micromodal-trigger="demo-request">
                            Contact us
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with about_showcase}}
    <section class="main-showcase">
        <div class="container">
            <div class="main-showcase__wrapper">
                <div class="main-showcase__demo-about">
                    <h2>{{header}}</h2>
                </div>
                <div class="main-showcase__info-about">
                    <div class="main-showcase__info-text">
                        <h2>{{subtitle}}</h2>
                        <p>{{copy}}</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
{{/with}}


{{#with company_members}}
    <section class="members">
        <div class="container">
            <div class="members__header">
            </div>
            <div class="members__team">
                {{#each list_of_members}}
                    <div class="members__team-tile">
                        <div class="members__tile-img">
                            <img src="{{img_url}}">
                            <a class="members__tile-link" href="{{link}}">
                                <img src="../assets/img/linkedin-logo.svg">
                            </a>
                        </div>
                        <div class="members__tile-data">
                            <h3>{{name}}</h3>
                            <span>{{job}}</span>
                        </div>
                    </div>
                {{/each}}
            </div>
            <div class="members-investors">
                {{#each list_of_investors}}
                    <div class="members-investors__tile">
                        <a class="members-investors__tile-img" href="{{link}}">
                            <img src="{{img_url}}">
                        </a>
                        <div class="members-investors__tile-data">
                            <h3>{{name}}</h3>
                            <span>{{job}}</span>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </section>
{{/with}}


